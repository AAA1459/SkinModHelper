    <Anim id="loop" path="idle" delay="0.06" frames="0-20" goto="flash"/>
    <Anim id="flash" path="flash" delay="0.06"  frames="0-20" goto="loop"/>
  </flyFeather>
  
  <booster path="SkinTest/TestB/booster/" start="loop">
    <Justify x="0.5" y="0.5"/>
    <Loop id="loop" path="booster" delay="0.1" frames="0-4"/>
